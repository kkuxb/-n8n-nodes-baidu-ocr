{
	"name": "n8n-nodes-baidu-ocr",
	"version": "0.3.0",
	"description": "n8n community node for Baidu Cloud OCR API",
	"keywords": [
		"n8n-community-node-package",
		"n8n",
		"ocr",
		"baidu",
		"paddleocr"
	],
	"license": "MIT",
	"homepage": "https://github.com/kkuxb/-n8n-nodes-baidu-ocr",
	"author": {
		"name": "kkuxb"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/kkuxb/-n8n-nodes-baidu-ocr.git"
	},
	"main": "index.js",
	"scripts": {
		"build": "tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials package.json",
		"lintfix": "eslint nodes credentials package.json --fix",
		"prepublishOnly": "npm run build"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/BaiduOcrApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/BaiduOcr/BaiduOcr.node.js"
		]
	},
	"devDependencies": {
		"@types/node": "^20.10.0",
		"@typescript-eslint/parser": "^6.0.0",
		"eslint": "^8.56.0",
		"gulp": "^4.0.2",
		"n8n-workflow": "^1.0.0",
		"prettier": "^3.1.0",
		"typescript": "^5.3.0"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	},
	"dependencies": {}
}
